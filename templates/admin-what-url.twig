{% extends 'main.twig' %}

{% block title %}Admin - Update Content{% endblock %}


{% block heading %}Update What We Do Info{% endblock %}

{% block admin %}
	
	<div class="row">
		
		<div class=".col-md-6 col-md-offset-3 for">

			<div class="panel panel-default">
				
				<div class="panel-heading">What We do Info Part 1</div>

				<div class="panel-body">
					
					<form action="{{ path_for('adminWhatUrl.update') }}" method="post" autcomplete="off">
						
						
                        <div class="form-group{{ errors.email ? ' has-error' : '' }}">
							
							<label for="what_img">What We Do Image Url</label>

							<input type"text" name="what_img" id="what_img" class="form-control" >


						</div>
                        
                        <button type="submit" class="btn btn-default">Update What We Do Info Part 1</button>

						{{ csrf.field | raw }}                       
                        
                        
                    </form>

                </div>

                </div>

        </div>

    </div>

{% endblock %}
